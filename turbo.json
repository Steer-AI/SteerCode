{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["VITE_PUBLIC_ENV", "SENTRY_AUTH_TOKEN", "SENTRY_PROJECT_NAME", "BACKEND_PORT"],
      "outputs": ["dist/**", ".svelte-kit/**",  ".vercel/**"]
    },
    "format": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
