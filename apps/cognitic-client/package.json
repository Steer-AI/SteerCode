{
  "type": "module",
  "name": "cognitic-client",
  "version": "0.0.1",
  "main": "./src/electron.cjs",
  "private": false,
  "scripts": {
    "dev": "vite dev --open",
    "dev:vercel": "vercel dev",
    "build": "yarn partytown && vite build",
    "build:vercel": "yarn build && rimraf ./.vercel/output/**/*.js.map",
    "electron": "wait-on http://localhost:5173 && electron ./src/electron.cjs",
    "electron-dev": "concurrently \"yarn run dev\" \"yarn run electron\"",
    "preelectron-pack": "yarn run build",
    "electron-pack": "electron-builder",
    "partytown": "partytown copylib static/~partytown",
    "preview": "vite preview",
    "format:changes": "git diff --cached --name-only --diff-filter=ACM | grep '\\.\\(js\\|ts\\|svelte\\|css\\|html\\)$' | sed 's/apps\\/cognitic-client/./g' | xargs prettier --ignore-path=../../.prettierignore --config=../../.prettierrc --write",
    "lint": "prettier --ignore-path=../../.prettierignore --config=../../.prettierrc --check . && eslint \"./src\"",
    "format": "prettier --ignore-path=../../.prettierignore --config=../../.prettierrc --write .",
    "test": "jest --passWithNoTests",
    "build-fs-operations": "tsc src/lib/data/src/*.ts --outDir src/lib/data/"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.7.0",
    "@formkit/auto-animate": "^1.0.0-beta.6",
    "@metamask/jazzicon": "^2.0.0",
    "@popperjs/core": "^2.11.6",
    "@rgossiaux/svelte-headlessui": "^1.0.2",
    "@segment/analytics-next": "^1.51.6",
    "@sentry/browser": "^7.44.2",
    "@sentry/svelte": "^7.16.0",
    "@sentry/sveltekit": "^7.44.2",
    "@sentry/tracing": "^7.16.0",
    "@sentry/vite-plugin": "^0.4.0",
    "@sveltejs/adapter-vercel": "^2.4.3",
    "@sveltejs/kit": "^1.15.8",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest": "^29.5.1",
    "@types/js-cookie": "^3.0.3",
    "@types/uuid": "^9.0.1",
    "@vercel/edge-config": "^0.1.5",
    "autoprefixer": "^9.8.8",
    "cognitic-models": "*",
    "concurrently": "^8.0.1",
    "cssnano": "^6.0.0",
    "dotenv": "^16.0.1",
    "electron": "^24.3.1",
    "electron-builder": "^23.6.0",
    "eslint-config-custom": "*",
    "firebase": "^9.21.0",
    "jest": "^29.5.0",
    "js-cookie": "^3.0.5",
    "node-machine-id": "^1.1.12",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "svelte": "^3.58.0",
    "svelte-popperjs": "^1.3.2",
    "svelte-preprocess": "^5.0.3",
    "svelte-virtual-scroll-list": "^1.1.0",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "vite": "^4.3.1",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "diff2html": "^3.4.35",
    "electron-serve": "^1.1.0",
    "file-icons-js": "websemantics/file-icons-js",
    "highlight.js": "^11.7.0",
    "sse.js": "^0.6.1",
    "svelte-i18n": "^3.6.0",
    "svelte-markdown": "^0.2.3"
  }
}
