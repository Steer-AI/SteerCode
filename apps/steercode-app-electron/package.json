{
  "type": "module",
  "name": "steercode-app",
  "version": "0.1.0",
  "main": "./src/electron.cjs",
  "private": false,
  "scripts": {
    "dev": "concurrently -n=svelte,electron -c='#ff3e00',blue \"yarn dev:svelte\" \"yarn dev:electron\"",
    "dev:svelte": "vite dev",
    "dev:electron": "electron src/electron.cjs",
    "build": "cross-env NODE_ENV=production yarn build:svelte && yarn build:electron",
    "build:svelte": "yarn partytown && vite build",
    "build:electron": "electron-builder -mwl --config build.config.json",
    "partytown": "partytown copylib static/~partytown",
    "format:changes": "git diff --cached --name-only --diff-filter=ACM | grep '\\.\\(js\\|ts\\|svelte\\|css\\|html\\)$' | sed 's/apps\\/cognitic-client/./g' | xargs prettier --ignore-path=../../.prettierignore --config=../../.prettierrc --write",
    "lint": "prettier --ignore-path=../../.prettierignore --config=../../.prettierrc --check . && eslint \"./src\"",
    "format": "prettier --ignore-path=../../.prettierignore --config=../../.prettierrc --write .",
    "test": "jest --passWithNoTests",
    "build-fs-operations": "rm src/lib/data/*.cjs && tsc --downlevelIteration src/lib/data/src/*.ts --outDir src/lib/data/ && ./rename_js_to_cjs.sh"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.7.0",
    "@metamask/jazzicon": "^2.0.0",
    "@popperjs/core": "^2.11.6",
    "@rgossiaux/svelte-headlessui": "^1.0.2",
    "@segment/analytics-next": "^1.51.6",
    "@sentry/browser": "^7.44.2",
    "@sentry/svelte": "^7.16.0",
    "@sentry/sveltekit": "^7.44.2",
    "@sentry/tracing": "^7.16.0",
    "@sentry/vite-plugin": "^0.4.0",
    "@sveltejs/adapter-static": "2.0.1",
    "@sveltejs/kit": "^1.15.8",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest": "^29.5.1",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^9.8.8",
    "cognitic-models": "*",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.0",
    "dotenv": "^16.0.1",
    "electron": "23.2.0",
    "electron-builder": "23.6.0",
    "electron-connect": "^0.6.3",
    "electron-packager": "^17.1.1",
    "electron-updater": "^5.3.0",
    "eslint-config-custom": "*",
    "jest": "^29.5.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "svelte": "^3.58.0",
    "svelte-popperjs": "^1.3.2",
    "svelte-preprocess": "^5.0.3",
    "svelte-virtual-scroll-list": "^1.1.0",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1"
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "node-machine-id": "^1.1.12",
    "diff2html": "^3.4.35",
    "electron-context-menu": "^3.6.1",
    "electron-reloader": "^1.2.3",
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "file-icons-js": "websemantics/file-icons-js",
    "highlight.js": "^11.7.0",
    "ignore": "^5.2.4",
    "sse.js": "^0.6.1",
    "svelte-i18n": "^3.6.0",
    "svelte-markdown": "^0.2.3"
  }
}
